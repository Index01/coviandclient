<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#EE007B</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:itemBackground">@color/backgroundMenu</item>
    </style>

    <style name="themeOLLight" parent="AppTheme">
        <item name="android:textColor">@android:color/black</item>
        <item name="colorPrimary">@color/bgPrimaryLight</item>
        <item name="colorSecondary">@color/colorSecondaryLight</item>
        <item name="colorPrimaryDark">@color/colorSecondaryLight</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="backgroundColor">@color/bgPrimaryLight</item>
        <item name="colorPrimaryVariant">@color/feedbackLight</item>
    </style>

    <style name="themeOLDark" parent="AppTheme">
        <item name="android:textColor">@android:color/white</item>
        <item name="colorPrimary">@color/bgPrimaryDark</item>
        <item name="colorSecondary">@color/colorSecondaryDark</item>
        <item name="colorPrimaryDark">@color/colorSecondaryDark</item>
        <item name="colorAccent">@color/colorAccentDark</item>
        <item name="backgroundColor">@color/bgPrimaryDark</item>
        <item name="colorPrimaryVariant">@color/feedbackDark</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>